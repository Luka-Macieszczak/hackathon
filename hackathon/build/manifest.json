{
  "short_name": "Extension",
  "version":"1.0",
  "name": "Ebay Price Extension",
  "manifest_version": 3,
  "action":{
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": ["https://amazon.com/*/*"],
      "js": ["content.js"]
    }
  ]
}